{
  "name": "teller-mvp",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "concurrently 'npm run watch' 'npm run serve'",
    "serve": "python -m SimpleHTTPServer",
    "watch": "concurrently 'npm run watch-js' 'npm run watch-css'",
    "watch-js": "nodemon --exec 'npm run build-js' --watch public/*.js",
    "watch-css": "nodemon --exec 'npm run build-css' --watch public/styles -e css",
    "build-js": "babel ./public/app.js --presets babel-preset-es2015 -o ./dist/bundle.js && uglifyjs ./dist/bundle.js -o ./dist/bundle.js --source-map ./dist/bundle.map --source-map-url bundle.map",
    "build-css": "cleancss ./public/styles/index.css --output ./dist/styles/styles.min.css",
    "build": "concurrently 'npm run build-css' 'npm run build-js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/troy-lamerton/data-filtering-tool.git"
  },
  "keywords": [
    "angular",
    "angularfire",
    "firebase",
    "tool"
  ],
  "author": "Troy Lamerton, Rory Wagner",
  "license": "UNLICENSE",
  "dependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-plugin-uglify": "^1.0.2",
    "babel-preset-es2015": "^6.18.0",
    "clean-css": "^3.4.23",
    "concurrently": "^3.1.0"
  }
}
